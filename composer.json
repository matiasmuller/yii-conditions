        "activequery",
        "db",
        "model"
    ],
    "homepage": "https://gitlab.com/puntogap/yii-conditions",
    "license": "MIT",
    "authors": [
        {
            "name": "Matias Muller",
            "email": "matias.muller@hotmail.com",
            "homepage": "https://github.com/puntogap",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4|~7.0"
    },
    "autoload": {
        "psr-4": {
